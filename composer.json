{
	"name": "hermajan/cornette",
	"description": "Extensions for Nette framework.",
	"type": "library",
	"homepage": "https://hermajan.net/cornette",
	"support": {
		"issues": "https://github.com/hermajan/cornette/issues"
	},
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Cornette\\": "src"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"require": {
		"php": ">=7.2",
		"contributte/translation": "^1.0",
		"nette/application": "^3.1",
		"nette/routing": "^3.0",
		"nettrine/orm": "^0.8.2"
	},
	"require-dev": {
		"nette/tester": "^2.4",
		"phpstan/phpstan": "^0.12.99",
		"phpstan/phpstan-nette": "^0.12.21"
	}
}
